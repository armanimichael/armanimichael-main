{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "jsx": "preserve",
    "lib": ["dom", "esnext"],
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "noUnusedLocals": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "./node_modules",
    "./node_modules/*",
    "public",
    "cache"
  ]
}
